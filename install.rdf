    <em:type>2</em:type> <!-- type: extension -->
    <em:unpack>true</em:unpack>

	<em:localized>
		<Description>
			<em:locale>en-US</em:locale>
			<em:name>Easy NZB Download</em:name>
			<em:description>Allows you to easyly find the NZB to a movie, from imdb, and send it to SABnzbd for download</Description>
	</em:localized>

	<em:localized>
		<Description>
			<em:locale>nl</em:locale>
			<em:name>Exchange EWS Provider</em:name>
			<em:description>Via een imdb pagina kun je makkelijk de bijbehorende NZB laten opzoeken en doorgeven aan SABnzbd voor download.</Description>
	</em:localized>

  </Description>
